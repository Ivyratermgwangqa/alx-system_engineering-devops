#!/usr/bin/env bash

# Ping an IP address 5 times

if [ -z "$1" ];
then
    echo "Usage: 5-is_the_host_on_the_network $0 {IP_ADDRESS}"
    exit 1
fi

ip_address="$1"

ping -c 5 "$ip_address"
